{
    "name": "laravel-auto/migrations",
    "description": "Automatic Laravel model migrations.",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Joseph Charika",
            "email": "joecharika@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": ">=3.0",
        "laravel/framework": ">=11.0"
    },
    "autoload": {
        "psr-4": {
            "Chareka\\AutoMigrate\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chareka\\AutoMigrate\\Providers\\AutoMigrateProvider"
            ]
        }
    }
}
